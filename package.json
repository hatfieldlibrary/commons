  "license": "MIT",
  "angular-cli": {},
  "author": {
    "name": "Michael Spalti",
    "email": "mspalti@gmail.com"
  },
  "keywords": [
    "angular7",
    "angular-cli",
    "pwa",
    "ngrx",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hatfieldlibrary/commons"
  },
  "engines": {
    "node": "8.13.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "build:development:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:production:ssr": "npm run build:client-and-server-bundles:prod && npm run webpack:server:prod",
    "build:dev:worker:ssr": "npm run build:dev:worker:prod && npm run webpack:server:prod",
    "serve:ssr": "HOST_ENV=development node dist/server",
    "build:client-and-server-bundles": "ng build --prod --base-href /commons/ && ng run commons:server",
    "build:client-and-server-bundles:prod": "ng build --prod --base-href /commons/ && ng run commons:server --configuration=production",
    "build:dev:worker:prod": "ng build --prod --base-href / && ng run commons:server:production",
    "webpack:server": "webpack --env.development --config webpack.server.config.js --progress --colors",
    "webpack:server:prod": "webpack --env.production --config webpack.server.config.js --progress --colors",
    "ng": "ng",
    "start:dev": "ng serve --host 0.0.0.0 --port 3005",
    "test": "ng test --code-coverage --watch --sourceMap=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "client:prod:build": "ng build --prod --deploy-url=/commons-resource/ --base-href /commons/",
    "server:prod:build": "tsc -p ./src/tsconfig.server.json",
    "build:prod": "ng test --watch=false && npm run client:prod:build && npm run server:prod:build && exit 0",
    "compodoc": "compodoc -p ./src/tsconfig.app.json",
    "build:demon": "nodemon --exec ts-node index.js",
    "client:build:watch": "ng build --watch",
    "build:dev:worker": "npm run build:dev:worker:ssr & npm run serve:ssr",
    "build:server:dev": "npm run client:build:watch & npm run server:prod:build & npm run serve:ssr",
    "docker:build": "npm run docker build . -t mspalti/commons:latest",
    "ci-test": "ng test --code-coverage --watch=false --sourceMap=false && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "scriptsComments": {
    "build:development:ssr": "Builds full application and server with development configs and deploys to dist.",
    "build:production:ssr": "Builds full application and server with production configs and deploys to dist.",
    "build:client-and-server-bundles": "Builds development client and server using Angular CLI",
    "build:client-and-server-bundles:prod": "Builds production client and server using Angular CLI",
    "serve:ssr": "Runs the development server in the local dist directory (includes ssr)",
    "build:dev:worker": "Builds a develpment client with server worker, an Express server, and starts server on port 3005.",
    "start:dev": "Uses Angular CLI to build and run development enviroment on port 3005 (no ssr, aot, or service workder)",
    "test": "Runs tests and watches for code updates."
  },
  "private": true,
