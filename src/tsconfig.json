  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "target": "es5",
    "module": "es2015",
    "lib": [
      "es2017",
      "dom"
    ],
    "typeRoots": [
      "../node_modules/@types"
    ]
  }
}
