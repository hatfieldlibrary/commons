{
  "version": 2,
  "index": "/collection/area/5",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://libmedia-test.willamette.edu/commons/**/*",
          "http://localhost:3005/**/*"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
          "https://libmedia-test.willamette.edu/commons/assets/**/*",
          "http://localhost:3005/assets/**/*"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-data",
      "urls": [
        "https://libmedia.willamette.edu/tagger-3-api/**/*"
      ],
      "cacheConfig": {
        "maxSize": 250,
        "maxAge": "1d",
        "version": 0,
        "timeout": "2s",
        "strategy": "freshness"
      }
    }
  ]
}
